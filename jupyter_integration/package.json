{
  "name": "@dharpa/react-proto-as-extension",
  "version": "0.1.0",
  "description": "minimal lab example",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://example.com",
  "license": "BSD-3-Clause",
  "author": "Project Jupyter Contributors",
  "files": [
    "src/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf,css,scss}",
    "src/react-proto-src/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf,css,scss}",
    "style/**/*.{eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "src/index.ts",
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:all": "jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm run build:lib && jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "xxxclean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:labextension": "rimraf jupyterlab_examples_kernel_messaging/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "xxxinstall:extension": "jupyter labextension develop --overwrite .",
    "xxprepare": "jlpm run clean && jlpm run build:prod",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch --development True .",
    "watch:src": "tsc -w",
    "copy-files": "copyfiles -u 1 src/react-proto-src/**/*.scss src/react-proto-src/**/**/*.scss src/**/*.scss src/*.scss src/react-proto-src/**/*.css src/react-proto-src/**/**/*.css src/react-proto-src/**/**/**/*.css src/**/*.css lib/",
    "wp": "webpack",
    "wpw": "webpack --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.0-rc.15",
    "@jupyterlab/launcher": "^3.0.0-rc.15",
    "@jupyterlab/mainmenu": "^3.0.0-rc.15",
    "@jupyterlab/nbformat": "^3.0.0-rc.15",
    "@jupyterlab/translation": "^3.0.0-rc.15",
    "@lumino/algorithm": "^1.3.3",
    "@lumino/coreutils": "^1.5.3",
    "@lumino/datagrid": "^0.5.2",
    "@lumino/disposable": "^1.4.3",
    "@lumino/widgets": "^1.16.1",
    "axios": "^0.21.1",
    "diff": "^5.0.0",
    "framer-motion": "^3.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-icons": "^4.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.1",
    "uuid": "^8.3.1",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@jupyterlab/builder": "^3.0.0-rc.15",
    "@types/node": "^14.14.22",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jsdoc": "^22.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "mini-css-extract-plugin": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "source-map-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "typescript": "~4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.18.0"
  },
  "sideEffects": [
    "style/*.css",
    "*.css",
    "*.scss"
  ],
  "jupyterlab": {
    "extension": true,
    "outputDir": "react_proto_as_extension/labextension",
    "webpackConfig": "./webpack.config.labext.js"
  }
}
